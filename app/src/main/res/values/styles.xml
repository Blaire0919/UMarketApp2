<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!-- ButtonStyle -->
    <style name="ButtonStyle01" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="backgroundTint">@color/NewUMarketDarkBlue</item>
        <item name="android:textColor">@color/white</item>
        <item name="elevation">15dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:padding">10dp</item>

    </style>

    <!--OutlineBoxStyle-->
    <style name="InputLayoutOutlineBox" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="boxStrokeColor">@color/black</item>
        <item name="tooltipForegroundColor">@color/black</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxCornerRadiusBottomEnd">25dp</item>
        <item name="boxCornerRadiusBottomStart">25dp</item>
        <item name="boxCornerRadiusTopEnd">25dp</item>
        <item name="boxCornerRadiusTopStart">25dp</item>
        <item name="elevation">10dp</item>
    </style>

    <style name="EditText01">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="errortest">
        <item name="android:textColor">@color/mustard</item>
        <item name="boxStrokeColor">@color/mustard</item>
    </style>

    <style name="DropDownOutlineBox" parent="Widget.MaterialComponents.TextInputLayout.FilledBox.Dense.ExposedDropdownMenu">
        <item name="boxStrokeColor">@color/black</item>
        <item name="backgroundColor">@color/transparent</item>
    </style>

    <!--User Profile InputLayout Style -->
    <style name="UserProfileInputLayout">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="boxStrokeColor">@color/black</item>

    </style>

    <!--Alert Dialog Style-->
    <style name="AlertDialogCustom" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">15sp</item>
    </style>

</resources>
